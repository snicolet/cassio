    sq1,sq2 : SInt32;
    my_bits, opp_bits : demi_bitboard;
    bitCaseVide : UInt32;
    leadingZeros : UInt32;
    leadingZeros2 : UInt32;
    nbIterations : SInt32;
    k, ticks, tempsDeCassio, tempsDeZebra : SInt32;
begin

  EcritBitboardState('compararaison des deux mthodes :',
		                      MakeBitboard(my_bits_low,my_bits_high,opp_bits_low,opp_bits_high), 2,alpha,beta,diffPions);



  nbIterations := 10000000;


  // la maniere de Cassio


  ticks := TickCount;

  for k := 1 to nbIterations do
    begin
      scoreVersionCassio := DeuxCasesVidesBitboardFast_DE_CASSIO( my_bits_low,my_bits_high,opp_bits_low,opp_bits_high, alpha, beta, diffPions, listeBitboard);
    end;

  tempsDeCassio := TickCount - ticks;



  // la maniere de Zebra

  ticks := TickCount;

  GET_DEUX_DERNIERES_CASES_VIDES_FROM_LISTE(listeBitboard, sq1, sq2);

  for k := 1 to nbIterations do
    begin

        begin
          my_bits.low   := my_bits_low;
          my_bits.high  := my_bits_high;
          opp_bits.low  := opp_bits_low;
          opp_bits.high := opp_bits_high
        end;

      scoreVersionZebra := solve_two_empty(my_bits, opp_bits, sq1, sq2, alpha, beta, diffPions, 1 );

    end;

  tempsDeZebra := TickCount - ticks;

  WritelnStringAndCoupDansRapport('sq1 = ',sq1);
  WritelnStringAndCoupDansRapport('sq2 = ',sq2);

  WritelnNumDansRapport('scoreVersionCassio = ',scoreVersionCassio);
  WritelnNumDansRapport('scoreVersionZebra = ',scoreVersionZebra);
  WritelnDansRapport('');
  WritelnNumDansRapport('temps de Cassio : ',tempsDeCassio);
  WritelnNumDansRapport('temps de Zebra : ',tempsDeZebra);

  AttendFrappeClavier;

  DeuxCasesVidesBitboardFast := scoreVersionCassio;

end;
